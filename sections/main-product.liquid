</div>

<script>
  document.addEventListener('DOMContentLoaded', function () {
    const stickyCart = document.getElementById('sticky-cart');
    const scrollThreshold = 1000;
    let isVisible = false;

    function handleScroll() {
      const scrollY = window.scrollY;

      if (scrollY >= scrollThreshold && !isVisible) {
        // Mostrar o cart
        stickyCart.classList.remove('tw-translate-y-full', 'tw-opacity-0');
        stickyCart.classList.add('tw-translate-y-0', 'tw-opacity-100');
        isVisible = true;
      } else if (scrollY < scrollThreshold && isVisible) {
        // Esconder o cart - FORÃ‡A as classes para garantir transiÃ§Ã£o suave
        stickyCart.classList.remove('tw-translate-y-0', 'tw-opacity-100');
        stickyCart.classList.add('tw-translate-y-full', 'tw-opacity-0');
        isVisible = false;
      }
    }

    // VersÃ£o com requestAnimationFrame mais controlada
    let ticking = false;

    function requestTick() {
      if (!ticking) {
        ticking = true;
        requestAnimationFrame(() => {
          handleScroll();
          ticking = false;
        });
      }
    }

    window.addEventListener('scroll', requestTick);
  });
</script>

<script>
  document.addEventListener('DOMContentLoaded', function () {
    const addToCartButtons = document.querySelectorAll('.custom-product-form');

    addToCartButtons.forEach((button) => {
      button.addEventListener('click', () => {
        window.scrollTo({
          top: 300,
          behavior: 'smooth',
        });
      });
    });
  });
</script>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    // URLs dos vÃ­deos
    const videoUrls = {
      'video-01': 'https://cdn.shopify.com/videos/c/o/v/2af2ab8f13f64420b03b43a8d84f7c7c.mp4',
      'video-02': 'https://cdn.shopify.com/videos/c/o/v/b762a40af53040ccbc29f290b41b5a2b.mp4',
      'video-03': 'https://cdn.shopify.com/videos/c/o/v/c6342e454bd44c8cb26cabc65f3abd25.mp4',
      {% comment %} How it Works {% endcomment %}
      'video-04': 'https://groov.me/cdn/shop/t/73/assets/groov.mp4?v=98447544743343531271750155813',
      'promo-video': "{{ 'promo-video.mp4' | asset_url }}"
    };
  
    // Criar o overlay do popup
    const overlay = document.createElement('div');
    overlay.className = 'video-popup-overlay';
    overlay.innerHTML = `
      <div class="video-popup-container">
        <button class="custom-btn-close">
          <svg viewBox="0 0 24 24">
            <path d="M19 6.41L17.59 5 12 10.59 6.41 5 5 6.41 10.59 12 5 17.59 6.41 19 12 13.41 17.59 19 19 17.59 13.41 12z"/>
          </svg>
        </button>
        <video controls autoplay>
          <source src="" type="video/mp4">
        </video>
      </div>
    `;
    document.body.appendChild(overlay);
  
    // Adicionar click nas divs com data-video
    document.addEventListener('click', function(e) {
      const videoDiv = e.target.closest('[data-video]');
      if (videoDiv) {
        const videoKey = videoDiv.getAttribute('data-video');
        const videoUrl = videoUrls[videoKey];
        
        if (videoUrl) {
          const video = overlay.querySelector('video source');
          const videoElement = overlay.querySelector('video');
          
          video.src = videoUrl;
          videoElement.load();
          overlay.classList.add('active');
          document.body.style.overflow = 'hidden';
        }
      }
    });
  
    // Fechar popup
    function closePopup() {
      overlay.classList.remove('active');
      document.body.style.overflow = '';
      const video = overlay.querySelector('video');
      video.pause();
      video.currentTime = 0;
    }
  
    // Event listeners para fechar
    overlay.querySelector('.custom-btn-close').addEventListener('click', closePopup);
    
    overlay.addEventListener('click', function(e) {
      if (e.target === overlay) {
        closePopup();
      }
    });
  });
</script>

